      <groupId>com.cattsoft</groupId>
      <artifactId>jconn4</artifactId>
      <version>1.0</version>
      <optional>true</optional>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>com.microsoft</groupId>
      <artifactId>jdbc</artifactId>
      <version>1.0.0</version>
      <scope>runtime</scope>
      <optional>true</optional>
    </dependency>
    
