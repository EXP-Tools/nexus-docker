        <version>1.0-SNAPSHOT</version>
    </parent>

    <groupId>com.catt</groupId>
    <artifactId>common-module-attachment</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>

    <modules>
        <module>common-module-attachment-data</module>
        <module>common-module-attachment-repository-entity</module>
        <module>common-module-attachment-repository-dao</module>
        <module>common-module-attachment-repository-dao-impl</module>
        <module>common-module-attachment-service</module>
        <module>common-module-attachment-service-impl</module>
